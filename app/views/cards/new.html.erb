<h1>New Card</h1>
<%= form_for :card, url: cards_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label 'Link To Card Image' %><br>
    <%= f.text_field :image %>
  </p>

   <p>
    <%= f.label 'Link To Card' %><br>
    <%= f.text_field :link %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', cards_path %>