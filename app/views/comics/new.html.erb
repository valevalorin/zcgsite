<h1>New Comic</h1>
<%= form_for :comic, url: comics_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label 'Image File' %><br>
    <%= f.text_field :image %>
  </p>

   <p>
    <%= f.label :description %><br>
    <%= f.text_area :desc %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', comics_path %>